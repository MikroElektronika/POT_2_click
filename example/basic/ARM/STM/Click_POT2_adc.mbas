module Click_POT2_adc

include Click_POT2_types
sub procedure pot2_adcInit() 
sub procedure pot2_adcSetInputChannel() 
sub function  pot2_adcRead() as uint32_t 

implements

sub procedure pot2_adcInit() 
    ADC1_Init() 
end sub

sub procedure pot2_adcSetInputChannel() 
    ADC_Set_Input_Channel(_ADC_CHANNEL_4) 
end sub

sub function  pot2_adcRead() as uint32_t 
dim
    adcData as uint32_t 

    adcData = ADC1_Read(4) 
    result = adcData 
end sub

end.