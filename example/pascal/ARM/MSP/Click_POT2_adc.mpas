unit Click_POT2_adc;

uses Click_POT2_types;

procedure pot2_adcInit(); 
procedure pot2_adcSetInputChannel(); 
function  pot2_adcRead() :  uint32_t; 

implementation

procedure pot2_adcInit(); 
begin
    ADC_Init(); 
end;

procedure pot2_adcSetInputChannel(); 
begin
    ADC_Set_Input_Channel(_ADC_CHANNEL_15); 
end;

function  pot2_adcRead() :  uint32_t; 
var
    adcData : uint32_t; 
begin
    adcData := ADC_Read(15); 
    result := adcData; 
end;

end.